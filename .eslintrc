{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "flowtype"
  ],
  "extends": [
    "uber-jsx",
    "uber-es2015",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "guard-for-in": 0,
    "no-inline-comments": 0,
    "no-invalid-this": 0,
    "react/forbid-prop-types": 0,
    "react/no-deprecated": 0,
    "flowtype/define-flow-type": "error",
    "flowtype/use-flow-type": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}
