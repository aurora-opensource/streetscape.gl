{
  "name": "streetscape.gl-kitti-demo",
  "description": "A demo of streetscape.gl with KITTI data",
  "version": "0.0.1",
  "scripts": {
    "start-local": "webpack-dev-server --env.local --progress --hot --open",
    "start": "webpack-dev-server --progress --hot --open",
    "build-clean": "rm -rf dist && mkdir dist",
    "build-copy": "cp index.html dist",
    "build-script": "NODE_ENV=production webpack --env.prod=true",
    "build": "npm run build-clean && npm run build-script && npm run build-copy"
  },
  "dependencies": {
    "math.gl": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "streetscape.gl": "^0.0.1",
    "webworkify-webpack": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.0.7",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.1"
  }
}
