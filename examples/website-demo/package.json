{
  "name": "streetscape.gl-website-demo",
  "description": "A demo of streetscape.gl",
  "version": "0.0.1",
  "scripts": {
    "start-local": "webpack-dev-server --env.local --progress --hot --open",
    "build-clean": "rm -rf dist && mkdir dist",
    "build-copy": "cp -r assets dist && cp index.html dist",
    "build-script": "NODE_ENV=production webpack  --env.local --env.prod",
    "build": "npm run build-clean && npm run build-script && npm run build-copy"
  },
  "dependencies": {
    "@loaders.gl/obj": "^1.0.0",
    "bowser": "^2.1.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "streetscape.gl": "^1.0.0-beta"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^0.28.11",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.1"
  }
}
